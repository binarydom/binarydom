{
  "name": "binary-dom",
  "version": "1.0.0",
  "description": "A highly performant alternative to Virtual DOM based on binary tree structure",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve",
    "build": "cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "analyze": "cross-env NODE_ENV=production webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "size": "size-limit",
    "size:track": "size-limit --track"
  },
  "keywords": [
    "dom",
    "virtual-dom",
    "binary-tree",
    "performance"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@size-limit/preset-big-lib": "^11.2.0",
    "@size-limit/webpack": "^11.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.3",
    "compression-webpack-plugin": "^11.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.7",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.57.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.1.6",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.5",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "dependencies": {
    "xxhash-wasm": "^1.0.2"
  }
}
